
    <!-- header section strats -->
    <header class="header_section">
      <nav class="navbar navbar-expand-lg custom_nav-container ">
        <a class="navbar-brand" href="/">
          <span>
            Giftos
          </span>
        </a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
          <span class=""></span>
        </button>

        <div class="collapse navbar-collapse" id="navbarSupportedContent">
          <ul class="navbar-nav  ">
            <li class="nav-item <%= k1 %>">
              <a class="nav-link" href="/">Home <span class="sr-only">(current)</span></a>
            </li>
            <li class="nav-item <%= k2 %>">
              <a class="nav-link" href="/shop">
                Shop
              </a>
            </li>
            <li class="nav-item <%= k3 %>">
              <a class="nav-link" href="/why">
                Why Us
              </a>
            </li>
            <li class="nav-item <%= k4 %>">
              <a class="nav-link" href="/test">
                Testimonial
              </a>
            </li>
            <li class="nav-item <%= k5 %>">
              <a class="nav-link" href="/contact">Contact Us</a>
            </li>
          </ul>
          <div class="user_option">
          <% if (user.id === undefined ){ %> 
            <a href="/login">
              <i class="fa fa-user" aria-hidden="true"></i>
              <span>
                Login
              </span>
            </a>
            <%} else {%>
              <i class="fa fa-user" aria-hidden="true" style="margin-right: 25px;"></i>
              
            <%} %>
            <a onclick="showgiohang()" type="button" data-bs-toggle="offcanvas" data-bs-target="#offcanvasMenu"
                aria-controls="offcanvasRight">
                <i class="fa fa-shopping-bag"></i>
              </a>
           
            <form class="form-inline form-navbar my-2 my-lg-0 order-2" action="/search" method="GET" >
              <button type="button" class="btn btn-search" onclick="toggleSearch()">
                  <i id="search-icon" class="fa fa-search" aria-hidden="true"></i>
              </button>
              <input id="search-input" class="form-control" name="search" type="text" placeholder="Search" >
          </form>
         
          
            
          </div>
        </div>
      </nav>
     
    
      
      
      
    
      <!-- Empty cart (remove `.d-none` to enable it) -->
      
    
      <div class="offcanvas offcanvas-end offcanvas-custom-zindex overflow-auto" tabindex="-1" id="offcanvasMenu">
        <div class="offcanvas-header justify-content-between border-bottom px-3">
            <h5 class="mb-0">Your Cart</h5>
            <button type="button" class="btn-close text-reset" data-bs-dismiss="offcanvas" aria-label="Close"></button>
        </div>
        <div id="cartItem" class="offcanvas-body d-flex flex-column px-3">
            <!-- Cart items will be added here -->
        </div>
        <div class="d-grid gap-2 col-12 mb-3">
            <div class="row">
                <div class="col-6">
                    <p>Subtotal:</p>
                </div>
                <div class="col-6 text-end">
                    <p id="subtotal">$0.00</p>
                </div>
            </div>
            <button class="btn btn-lg btn-dark w-100 rounded-pill" type="button">Check</button>
        </div>
    </div>
    </header>